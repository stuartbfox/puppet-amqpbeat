{
  "name": "stuartbfox-amqpbeat",
  "version": "0.0.1",
  "author": "stuartbfox",
  "license": "Apache-2.0",
  "summary": "A module to install and manage the amqpbeat message shipper",
  "source": "https://github.com/stuartbfox/puppet-amqpbeat",
  "project_page": "https://github.com/stuartbfox/puppet-amqpbeat",
  "issues_url": "https://github.com/stuartbfox/puppet-amqpbeat/issues",
  "tags": ["amqpbeat", "amqp", "logstash", "elasticsearch", "elastic"],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.6.0 <5.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">=2.0.0 <3.0.0"
    },
    {
      "name": "lwf/remote_file",
      "version_requirement": ">= 0.2.0 < 2.0.0"
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.0.0 < 2015.4.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.0.0 < 5.0.0"
    }
  ]
}
